@model DoctorAppointments.Controllers.AppointmentsController
@{
    ViewBag.Title = "RequestAmka";
}


<h2>Request Amka</h2>


<form action="/Appointments/PreviousAppointments/" method="get">
    <div class="form-group sow">
        <label>Patient AMKA</label>
        <input type="text" class="form-control" id="amka" name="amka" placeholder="Your amka..">
    </div>


    <div class="form-group">
        <button type="submit" class="btn btn-success">Show appointments</button>
    </div>

</form>